<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p" 
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <!-- 采用注解方式，无须配置bean -->
    <context:component-scan base-package="com.jemmyee.framework.dao"/>
	<context:component-scan base-package="com.jemmyee.cms.dao"/>
	<context:component-scan base-package="com.jemmyee.member.dao"/>
	<context:component-scan base-package="com.jemmyee.taoke.dao"/>
</beans>